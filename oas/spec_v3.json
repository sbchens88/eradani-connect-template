                "parameters": [
                    {
                        "name": "num",
                        "in": "path",
                        "required": true,
                        "example": 5,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "input": {
                                            "type": "number",
                                            "example": 5
                                        },
                                        "output": {
                                            "type": "number",
                                            "example": 500
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "304": {
                        "description": ""
                    },
                    "400": {
                        "description": "Bad Request",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Bad Data Supplied to API"
                                        },
                                        "errors": {
                                            "type": "object",
                                            "properties": {
                                                "num": {
                                                    "type": "object",
