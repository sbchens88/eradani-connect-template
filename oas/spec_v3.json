                                                        "value": {
                                                            "type": "string",
                                                            "example": "a"
                                                        },
                                                        "msg": {
                                                            "type": "string",
                                                            "example": "Invalid value"
                                                        },
                                                        "param": {
                                                            "type": "string",
                                                            "example": "num"
                                                        },
                                                        "location": {
                                                            "type": "string",
                                                            "example": "params"
                                                        }
                                                    }
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Unknown Error"
                                        },
                                        "data": {
                                            "example": null,
                                            "nullable": true
                                        }
                                    }
                                }
                            }
                        }
                    }
                },
                "tags": []
            }
        },
